{
    "name": "masterflash-ru/kabinet",
    "description": "Кабинет посетителя сайта",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash"
    ],
    "homepage": "https://masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "masterflash-ru/permissions": "*",
        "zendframework/zend-mvc-plugin-prg": "^1.0",
        "zendframework/zend-form": "^2.10",
        "zendframework/zend-cache": "^2.7",
        "zendframework/zend-inputfilter": "^2.7",
        "masterflash-ru/statpage": "^1.0",
        "masterflash-ru/validator_i18n": "^1.0",
        "zendframework/zend-captcha": "^2.7",
        "zendframework/zendservice-recaptcha": "^3.0",
        "zendframework/zend-mail": "^2.8"

    },
    "autoload": {
        "psr-4": {
            "Mf\\Kabinet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mf\\Kabinet\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev",
            "dev-develop": "0.0.1-dev"
        },
        "zf": {
            "component": "Mf\\Kabinet",
            "config-provider": "masterflash-ru\\Mf\\Kabinet\\ConfigProvider"
        }
    }
}
